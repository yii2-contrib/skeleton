{
  "name": "yii2-contrib/skeleton",
  "description": "The contrib project skeleton",
  "keywords": [
    "skeleton",
    "yii2",
    "yii2-skeleton"
  ],
  "homepage": "https://github.com/yii2-contrib/skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "lichunqiang",
      "email": "light-li@hotmail.com"
    }
  ],
  "require": {
    "php": "~5.6|~7.0"
  },
  "autoload": {
    "psr-4": {
      "YiiContrib\\Skeleton\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YiiContribTest\\Skeleton\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "php ./vendor/bin/phpunit"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0||~5.0||~6.0"
  }
}
